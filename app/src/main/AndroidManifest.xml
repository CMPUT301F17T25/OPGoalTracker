<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.ualberta.cs.opgoaltracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register_activity" />
        <activity
            android:name=".HabitActivity"
            android:label="@string/title_activity_habit" />
        <activity
            android:name=".HabitAddActivity"
            android:label="@string/title_activity_habit_add"/>
        <activity android:name=".HabitDetailActivity" ></activity>
    </application>

</manifest>